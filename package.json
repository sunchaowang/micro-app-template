{
  "name": "qiankun-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install": "npm-run-all --serial install:*",
    "install:main": "cd main && npm i",
    "install:microapp_system": "cd microapp-system && npm i",
    "install:microapp_operation": "cd microapp-operation && npm i && node node_modules/esbuild/install.js",
    "install:microapp_game": "cd microapp-game && npm i && node node_modules/esbuild/install.js",
    "start": "npm-run-all --parallel start:*",
    "start:main": "cd main && npm run dev",
    "start:microapp_system": "cd microapp-system && npm run dev",
    "start:microapp_operation": "cd microapp-operation && npm run dev",
    "start:microapp_game": "cd microapp-game && npm run dev"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
