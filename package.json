{
  "name": "qiankun-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install": "npm-run-all --serial install:*",
    "install:main": "cd main && npm i",
    "install:microapp-system": "cd microapp-system && npm i",
    "install:microapp-operation": "cd microapp-operation && npm i",
    "install:microapp-vue3-vuecli": "cd microapp-vue3-vuecli && npm i",
    "start": "npm-run-all --parallel start:*",
    "start:main": "cd main && npm run dev",
    "start:microapp-system": "cd microapp-system && npm run dev",
    "start:microapp-operation": "cd microapp-operation && npm run dev",
    "start:microapp-vue3-vuecli": "cd microapp-vue3-vuecli && npm run dev"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
